<h2>Categories</h2>
<br>
<% @categories.each do |category|  %>
<div class="itemhols">
    <h2><%= category.title %></h2>
    <%= link_to (image_tag category.image, :class=>"itemholsIm"), category %>
    <p><%= category.description %></p>
    <p><a href="/category/"<%= category.title %> </p>
    <%= link_to (category.title), category %>
</div>

<% end %>